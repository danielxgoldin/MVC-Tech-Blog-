<article class="post-title">
    <div class="post">
        <div class="info">
            <div class="name">
                <a href="/post/{{id}}" class="links">{{title}}</a>
            </div>
            <div class="made-by">
                <p id="comment-data">
                    Created By <b>{{user.username}} </b> Date <b>{{formatDate created_at}}</b>
                </p>
            </div>
        </div>
        <div class="content">
            <p>{{content}}</p>
        </div>
    </div>
</article>